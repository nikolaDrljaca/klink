CREATE TABLE IF NOT EXISTS klink_key (
    id uuid PRIMARY KEY DEFAULT gen_random_uuid(),
    read_key varchar(8)
        NOT NULL,
    write_key varchar(8)
        NOT NULL,
    klink_id uuid
        REFERENCES klink(id)
        ON DELETE CASCADE
);


-- queries
findByKlinkId:
SELECT *
FROM klink_key
WHERE klink_id = :klinkId;
