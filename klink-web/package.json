{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "prodlike": "vite build --mode prodlike",
    "copy-api-doc": "cp ../klink-api/api.yaml ./src/api.yaml",
    "clear-generated": "rm -rf ./src/generated",
    "openapi-gen": "npm run copy-api-doc && npm run clear-generated && openapi-generator-cli generate -i ./src/api.yaml -g typescript-fetch -o ./src/generated",
    "openapi-gen-only": "openapi-generator-cli generate -i ./src/api.yaml -g typescript-fetch -o ./src/generated"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.7.4",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.12.12",
    "postcss": "^8.4.33",
    "solid-devtools": "^0.29.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-solid": "^2.8.2"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.15.0",
    "@solid-primitives/clipboard": "^1.5.10",
    "@solid-primitives/event-bus": "^1.0.11",
    "@solid-primitives/resource": "^0.3.1",
    "@solid-primitives/share": "^2.0.6",
    "@solid-primitives/storage": "^4.2.1",
    "@solidjs/router": "^0.15.1",
    "@unpic/solid": "^0.0.49",
    "clsx": "^2.1.1",
    "localforage": "^1.10.0",
    "lucide-solid": "^0.461.0",
    "solid-js": "^1.8.11",
    "solid-toast": "^0.5.0"
  }
}
