{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "copy-api-doc": "cp ../klink-api/api.yaml ./src/api.yaml",
    "clear-generated": "rm -rf ./src/generated",
    "openapi-gen": "npm run copy-api-doc && npm run clear-generated && openapi-generator-cli generate -i ./src/api.yaml -g typescript-fetch -o ./src/generated",
    "openapi-gen-only": "openapi-generator-cli generate -i ./src/api.yaml -g typescript-fetch -o ./src/generated"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.7.4",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.12.12",
    "postcss": "^8.4.33",
    "solid-devtools": "^0.29.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-solid": "^2.8.2"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.15.0",
    "solid-js": "^1.8.11"
  }
}
